   "source": [
    "jrdb_host = \"https://jrdb.jlab.org/\"\n",
    "\n",
    "with open('token.txt', 'r') as TOK:\n",
    "    token = TOK.read()\n",
    "headers = {\n",
    "    'Authorization': f'Bearer {token}',\n",
    "    'Accept': 'application/json'  # Optional: specify the desired content type\n",
    "}\n",
    "url = f\"{jrdb_host}api/records\"\n",
    "\n",
    "length=100    \n",
    "page=1\n",
    "hallA=[]\n",
    "hallB=[]\n",
    "hallC=[]\n",
    "hallD=[]\n",
    "while length == 100 :\n",
    "    params = {\n",
    "    'q':'metadata.resource_type.id:publication-article metadata.resource_type.id:publication-thesis',  # Replace with your search query\n",
    "    'sort':'publication',  # Replace with your desired sorting option\n",
    "    'size': 100,  # Replace with the number of items per page\n",
    "    'page': page,  # Replace with the page number\n",
    "    'allversions': False  # Replace with True if you want all versions included\n",
    "    }\n",
    "    \n",
    "    Xresponse=requests.get(url, headers=headers, params=params, verify=False)\n",
    "    Xresult = Xresponse.json()\n",
    "    if Xresponse.status_code == 200:\n",
    "        Xrecords=Xresult['hits']['hits']\n",
    "        \n",
    "        for r in Xrecords:\n",
    "            if r['custom_fields']['rdm:division'][0]['title']['en'] == 'Exp Nuclear Physics / Experimental Halls / Hall D':\n",
    "                hallD.append(r)\n",
    "            elif r['custom_fields']['rdm:division'][0]['title']['en'] == 'Exp Nuclear Physics / Experimental Halls / Hall C':\n",
    "                hallC.append(r)\n",
    "            elif r['custom_fields']['rdm:division'][0]['title']['en'] == 'Exp Nuclear Physics / Experimental Halls / Hall B':\n",
    "                hallB.append(r)\n",
    "            elif r['custom_fields']['rdm:division'][0]['title']['en'] == 'Exp Nuclear Physics / Experimental Halls / Hall A':\n",
    "                hallA.append(r)\n",
    "        \n",
    "        page+=1\n",
    "        length=len(Xrecords)\n",
    "    else:\n",
    "        length=0\n",
    "print(len(hallD),len(hallC),len(hallB),len(hallA))\n",
    "print((len(hallD)+len(hallC)+len(hallB)+len(hallA)))\n",
    "JLab={'Hall A':hallA,'Hall B':hallB,'Hall C':hallC,'Hall D':hallD}\n",
    "print(page)\n",
    "with open(f'DataLog\\JLab_Papers_{date.today()}.pk1','wb') as file:\n",
    "    pickle.dump(JLab,file)\n",
    "with open('DataLog\\JLab_Papers_UPDATED.pk1','wb') as file:\n",
    "    pickle.dump(JLab,file)"
   ]
