    "\n",
    "We will also make the publications a little more manageable."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "73dd4f29",
   "metadata": {},
   "outputs": [],
   "source": [
    "MapDat={}\n",
    "for h in ['A','C']:\n",
    "    MapDat[f'Hall {h}']={}\n",
    "    with open(f'ExperimentSort\\Data\\Current\\Hall{h}.pk1','rb') as fi:\n",
    "        File = pickle.load(fi)\n",
    "        for ex in File:\n",
    "            pbs=[]\n",
    "            D=ex.split('E')\n",
    "            if len(D)>1:\n",
    "                nums=D[1].split('-')\n",
    "                num=''.join(nums)\n",
    "                if num in MEH:\n",
    "                    start = MEH[num][0]\n",
    "                    end = MEH[num][1]\n",
    "                    for p in File[ex]:\n",
    "                        if p['metadata']['resource_type']['id'] == 'publication-thesis':\n",
    "                            J='Thesis'\n",
    "                        else:\n",
    "                            J=p['custom_fields']['journal:journal']['title']\n",
    "                        date=p['metadata']['publication_date']\n",
    "                        pbs.append({'Journal':J,'Date':date,'Metadata':p})\n",
    "                    MapDat[f'Hall {h}'][ex] = {'Start':start,'End':end,'Publications':pbs}"
   ]
