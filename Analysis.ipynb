    "    \n",
    "for key in JLab:\n",
    "    orphanPubs=[]\n",
    "    k=[]\n",
    "    Theses=[]\n",
    "    Journals=[]\n",
    "    for a in JLab[key]:\n",
    "        if 'journal:journal' in a['custom_fields']:\n",
    "            if a['custom_fields']['journal:journal']['title'] not in BadJs:\n",
    "                k.append(a)\n",
    "        else:\n",
    "            k.append(a)\n",
    "    for p in k:\n",
    "        if 'rdm:experiment_number' not in p['custom_fields']:\n",
    "            orphanPubs.append(p)\n",
    "    Hall=[p for p in k if p not in orphanPubs]\n",
    "    \n",
    "    print(key)\n",
    "    Perc=len(Hall)/(len(k))\n",
    "    print(Perc)\n",
    "    for t in Hall:\n",
    "        if t['metadata']['resource_type']['id'] == 'publication-thesis':\n",
    "            Theses.append(t)\n",
    "        else:\n",
    "            Journals.append(t)\n",
    "    HallPubs[key]={'Housed':Hall,'Orphans':orphanPubs,'Percentage':Perc,'Journals':Journals,'Theses':Theses}\n",
