    "import requests\n",
    "import pickle\n",
    "from datetime import date\n",
    "import os\n",
    "import pandas as pd\n",
    "import time as t\n",
    "import numpy as np\n",
    "\n",
    "def date_to_months(date):\n",
    "    # Convert a date string in the form YYYY-MM to months since 0000-00\n",
    "    sep=date.split('-')\n",
    "    if len(sep)==2:\n",
    "        months=(12*int(sep[0]))+int(sep[1])\n",
    "    else:\n",
    "        months=(12*int(date))+.5\n",
    "    return months\n",
    "\n",
    "def months_to_date(months):\n",
    "    # Convert amount of months from the start of AD to a date in the form YYYY-MM\n",
    "    if months%12==0:\n",
    "        date=f'{int((months/12)-1)}-12'\n",
    "    else:\n",
    "        date=f'{int(math.floor(months/12))}-{int(months%12)}'\n",
    "    return date\n"
   ]
